// putsln: prints a string with terminating new line to stdout
fn putsln(string s) {
    puts(s);
    puts("\n");
}

// putiln: prints an integer with terminating new line to stdout
fn putiln(int64 x) {
    puti(x);
    puts("\n");
}

// pow: calculates `x` to the power of `power`
fn pow(int64 x, int64 power) int64 {
    let result: int64 = 1;
    while power > 0 {
        result = result * x;
        power = power - 1;
    }
    ret result;
}
